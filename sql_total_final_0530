--------------------------------------------------------
--  DDL for Function RADIANS. 먼저 정의하기.
--------------------------------------------------------

DECLARE
  FUNCTION RADIANS_EXISTS RETURN BOOLEAN IS
    v_count NUMBER;
  BEGIN
    SELECT COUNT(*) INTO v_count FROM USER_OBJECTS WHERE OBJECT_TYPE = 'FUNCTION' AND OBJECT_NAME = 'RADIANS';
    RETURN v_count > 0;
  END;
BEGIN
  IF NOT RADIANS_EXISTS THEN
    EXECUTE IMMEDIATE '
      CREATE OR REPLACE FUNCTION "EZEN"."RADIANS" (nDegrees IN NUMBER) 
      RETURN NUMBER DETERMINISTIC 
      IS
      BEGIN
        RETURN nDegrees / 57.29577951308232087679815481410517033235;
      END RADIANS;';
  END IF;
  
  EXECUTE IMMEDIATE '
    CREATE OR REPLACE FUNCTION "EZEN"."DISTNACE_WGS84" ( H_LAT IN NUMBER, H_LNG IN NUMBER, T_LAT IN NUMBER, T_LNG IN NUMBER)
    RETURN NUMBER DETERMINISTIC 
    IS
    BEGIN
      RETURN (
        6371.0 * ACOS(  
          COS( RADIANS( H_LAT ) ) * COS( RADIANS( T_LAT /* 위도 */ ) )
          * COS( RADIANS( T_LNG /* 경도 */ ) - RADIANS( H_LNG ) )
          +
          SIN( RADIANS( H_LAT ) ) * SIN( RADIANS( T_LAT /* 위도 */ ) )        
         ));
    END DISTNACE_WGS84;';
END;
/

